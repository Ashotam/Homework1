//Swap 2 variables.
   // With third variable
//With arithmetic operators
{
    let a = 2;
    let b = 4;
    let c = a;
    a = b;
    b= c;
    console.log(a,b)
}
{
   let a = 2;
   let b = 4;
   a = a + b;
    b = a - b;
    a = a - b;
    console.log(a,b)
}

